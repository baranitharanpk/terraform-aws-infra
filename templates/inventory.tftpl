# Ansible Inventory - Auto-generated by Terraform

[apache_servers]
%{ for idx, ip in apache_servers ~}
apache-${idx + 1} ansible_host=${ip} ansible_user=ubuntu
%{ endfor ~}

[nginx_servers]
%{ for idx, ip in nginx_servers ~}
nginx-${idx + 1} ansible_host=${ip} ansible_user=ubuntu
%{ endfor ~}

[webservers:children]
apache_servers
nginx_servers

[all:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
